{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "stub": {
      "dependsOn": ["^stub"],
      "outputs": ["dist"]
    },
    "@syseven/icons-vue#build": {
      "dependsOn": [],
      "inputs": ["src/**", "package.json", "tsconfig.build.json", "tsconfig.json"],
      "outputs": ["dist"]
    }
  }
}