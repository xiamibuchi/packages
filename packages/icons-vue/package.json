{
  "name": "@syseven/icons-vue",
  "private": "true",
  "description": "Vue components of Icons collection.",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "types": "./dist/types/global.d.ts",
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/types/*"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run build:generate && pnpm run build:build && pnpm run build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@pnpm/find-workspace-dir": "^6.0.0",
    "@syseven/icons-svg": "workspace:*",
    "unplugin-vue": "^4.0.2"
  }
}