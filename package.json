{
  "name": "yang-packages",
  "private": true,
  "version": "1.0.0",
  "description": "base utils and components",
  "main": "index.js",
  "packageManager": "pnpm@8.7.0",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "node ./scripts/preinstall.js",
    "postinstall": "pnpm stub",
    "stub": "turbo stub --parallel",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build": "pnpm run -C internal/build start",
    "docs:build": "pnpm --filter @syseven/docs build",
    "test": "pnpm run -r unit:test && vitest",
    "test:coverage": "vitest --coverage"
  },
  "lint-staged": {
    "*.{ts,js,vue,tsx,jsx,md}": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "ShenYang",
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "volta": {
    "node": "18.16.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/types": "^9.2.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@syseven/build-utils": "workspace:*",
    "@syseven/components": "workspace:*",
    "@syseven/constants": "workspace:*",
    "@syseven/eslint-config": "workspace:*",
    "@syseven/hooks": "workspace:*",
    "@syseven/locale": "workspace:*",
    "@syseven/style": "workspace:*",
    "@syseven/utils": "workspace:*",
    "@syseven/video-player": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/js-cookie": "3.0.6",
    "@types/jsdom": "^20.0.1",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.4.8",
    "@types/prettier": "^2.7.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "@vue/shared": "^3.4.21",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.9.0",
    "autoprefixer": "^10.4.18",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.46.0",
    "eslint-config-next": "13.4.19",
    "execa": "^7.2.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.2.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "less": "^4.2.0",
    "lint-staged": "15.2.2",
    "lodash-es": "^4.17.21",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "5.0.5",
    "rollup": "3.27.2",
    "rollup-plugin-esbuild": "^4.10.3",
    "sass": "1.71.1",
    "svgo": "3.2.0",
