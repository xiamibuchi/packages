{
  "name": "yang-packages",
  "private": true,
  "version": "1.0.0",
  "description": "base utils and components",
  "main": "index.js",
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "node ./scripts/preinstall.js",
    "postinstall": "pnpm stub && pnpm run build:icons",
    "stub": "turbo stub --parallel",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build": "pnpm run -C internal/build start",
    "build:icons": "turbo @syseven/icons-vue#build",
    "docs:build": "pnpm --filter @syseven/docs build",
    "test": "pnpm run -r unit:test && vitest",
    "test:coverage": "vitest --coverage"
  },
  "lint-staged": {
    "*.{ts,js,vue,tsx,jsx,md}": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "ShenYang",
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "volta": {
    "node": "18.16.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/types": "^9.2.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@syseven/build-utils": "workspace:*",
    "@syseven/components": "workspace:*",
    "@syseven/constants": "workspace:*",
    "@syseven/eslint-config": "workspace:*",
    "@syseven/hooks": "workspace:*",
    "@syseven/locale": "workspace:*",
    "@syseven/style": "workspace:*",
    "@syseven/utils": "workspace:*",
    "@syseven/video-player": "workspace:*",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.13",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.1",
    "@types/gulp-rename": "^2.0.2",
    "@types/gulp-sass": "^5.0.1",
    "@types/jsdom": "^20.0.1",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.4.8",
    "@types/prettier": "^2.7.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-v8": "^0.34.1",
    "@vitest/ui": "^0.31.4",
    "@vue/shared": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^10.3.0",
    "autoprefixer": "^10.4.14",
    "chalk": "^5.3.0",
    "components-helper": "^2.1.4",
    "consola": "^3.2.3",
    "dotenv": "^16.3.1",
    "esbuild": "^0.17.19",
    "eslint": "^8.46.0",
    "eslint-config-next": "13.4.19",
    "execa": "^7.2.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
