    "ts-morph": "^14.0.0",
    "tsx": "^3.12.7",
    "turbo": "^1.10.12",
    "type-fest": "^3.13.1",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vite": "^4.4.9",
